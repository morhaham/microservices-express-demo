{
  "name": "kafka",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/kafka",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "tsc"
        ],
        "cwd": "packages/kafka"
      }
    },
    "k8s:deploy:dev": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "sh scripts/k8s-dev.sh"
        ],
        "cwd": "packages/kafka"
      }
    }
  }
} 